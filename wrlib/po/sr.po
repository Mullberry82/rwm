msgstr "операција је заустављена"

#: ../../wrlib/load_webp.c:51
msgid "aborted by user"
msgstr "кориснички прекид"

#: ../../wrlib/load_webp.c:52
msgid "not enough data"
msgstr "недовољно података"

#: ../../wrlib/load_webp.c:61
#, c-format
